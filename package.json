{
  "name": "mirabow",
  "version": "0.0.51",
  "description": "A \"mirabow\" is a parser generator for parsing token sequences with a declarative parser.",
  "keywords": [
    "parser",
    "generator",
    "mirabow"
  ],
  "author": "tbsten",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TBSten/mirabow.git"
  },
  "homepage": "https://github.com/TBSten/mirabow",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist/cjs/*",
    "dist/esm/*"
  ],
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./index.d.ts"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "ts-node": "^10.9.1",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --p tsconfig.cjs.json",
    "build:esm": "tsc --p tsconfig.esm.json",
    "prepack": "npm run build",
    "test": "jest --silent=false --verbose false"
  }
}